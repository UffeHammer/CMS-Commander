<project>
  <Metadata>
    <metadata>
      <PackageName>Sitecore webservice installer</PackageName>
      <Author>Uffe Hammer</Author>
      <Version>1.0</Version>
      <Revision />
      <License />
      <Comment />
      <Attributes />
      <Readme />
      <Publisher />
      <PostStep />
      <PackageID />
