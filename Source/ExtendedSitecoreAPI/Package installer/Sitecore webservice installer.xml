  </Metadata>
  <SaveProject>True</SaveProject>
  <Sources>
    <files>
      <Include>
        <FileNameFilter>
          <Pattern>ExtendedSitecoreAPI.*</Pattern>
          <AcceptDirectories>False</AcceptDirectories>
        </FileNameFilter>
        <FileDateFilter>
          <FilterType>CreatedFilter</FilterType>
          <ActionDateTo />
          <ActionDateFrom />
