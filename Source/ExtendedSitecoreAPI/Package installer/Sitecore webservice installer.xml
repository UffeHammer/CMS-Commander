        <FileDateFilter>
          <FilterType>ModifiedFilter</FilterType>
          <ActionDateTo />
          <ActionDateFrom />
          <NotOlderThan />
        </FileDateFilter>
      </Include>
      <Exclude />
      <Name>ExtendedSitecoreAPI bin files</Name>
      <Root>/bin</Root>
      <Converter>
        <FileToEntryConverter>
          <Root>/</Root>
          <Transforms />
        </FileToEntryConverter>
      </Converter>
    </files>
    <xfiles>
      <Entries>
        <x-item>/sitecore modules/Shell/ExtendedSitecoreAPI/Security.asmx</x-item>
      </Entries>
      <Converter>
        <FileToEntryConverter>
          <Root>/</Root>
          <Transforms />
        </FileToEntryConverter>
      </Converter>
      <Include />
      <Exclude />
      <Name>ExtendedSitecoreAPI webservice asmx file</Name>
    </xfiles>
  </Sources>
  <Converter>
    <TrivialConverter>
      <Transforms />
    </TrivialConverter>
  </Converter>
  <Include />
  <Exclude />
  <Name />
